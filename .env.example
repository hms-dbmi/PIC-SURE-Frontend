VITE_GOOGLE_ANALYTICS_ID=someid
VITE_GOOGLE_TAG_MANAGER_ID=someid

VITE_AUTH_TOUR_NAME=NHANES-Auth
VITE_OPEN_TOUR_NAME=BDC-Open