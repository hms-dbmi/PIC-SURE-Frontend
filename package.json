    "svelte": "^4.2.19",
    "svelte-check": "^3.8.5",
    "svelte-eslint-parser": "^0.40.0",
    "tailwindcss": "3.3.6",
    "tslib": "^2.6.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-tailwind-purgecss": "0.3.0",
    "vitest": "^2.1.2"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
